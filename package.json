{
  "name": "react-transform-boilerplate",
  "version": "1.0.0",
  "description": "A new Webpack boilerplate with hot reloading React components, and error handling on module and component level.",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack2": "NODE_ENV=production; webpack --config webpack.config.prod.js",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "test": "webpack --progress --debug --config webpack.config.dev.js",
    "start": "node devServer.js",
    "dev-server": "webpack-dev-server --inline --hot --config webpack.config.dev.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-transform-boilerplate.git"
  },
  "author": "Dan Abramov <dan.abramov@me.com> (http://github.com/gaearon)",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/gaearon/react-transform-boilerplate/issues"
  },
  "homepage": "https://github.com/gaearon/react-transform-boilerplate",
  "devDependencies": {
    "autobind-decorator": "^1.3.4",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-material-ui": "^0.3.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-stage-0": "^6.22.0",
    "babel-root-slash-import": "^1.1.0",
    "eslint": "^3.15.0",
    "eslint-plugin-react": "^6.10.0",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "happypack": "^3.0.2",
    "material-ui": "^0.17.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.3",
    "redux-storage": "^4.1.2",
    "rimraf": "^2.6.0",
    "validator": "^6.2.1",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "dependencies": {
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "isomorphic-fetch": "^2.2.1",
    "rc-collapse": "^1.6.12",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-motion": "^0.4.7",
    "react-native-accordion": "^1.0.1",
    "react-redux": "^5.0.2",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-storage-decorator-debounce": "^1.1.3",
    "redux-storage-decorator-migrate": "^1.1.0",
    "redux-storage-engine-localstorage": "^1.1.4",
    "redux-thunk": "^2.2.0",
    "sanitize-html": "^1.14.1",
